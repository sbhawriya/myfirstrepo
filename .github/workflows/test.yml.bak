name: testing-copy-files
on:
  push: 
    branches: [ master ]
jobs:
  copy:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      - name: "create a new file"
        run: |
          touch newfile123
          ls -ld newfile123
  #    - name: Setup tmate session
  #      uses: mxschmitt/action-tmate@v3
  print:
    needs: copy 
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      - name: "printing the files"
        run: |
          mv test.txt test.txt123
      - name: Setup tmate session
        uses: mxschmitt/action-tmate@v3